@model List<Beer>

<h1>Cervezas</h1>
<a asp-action="Create">Create</a>

<table>
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Marca</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach(var beer in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m=>beer.Name)</td>
                <td>@Html.DisplayFor(m=>beer.Brand.Name)</td>
            </tr>
        }
    </tbody>
</table>